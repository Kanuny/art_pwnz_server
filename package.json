{
  "name": "art_pwnz_server",
  "version": "0.0.1",
  "description": "server for art_pwnz",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/app.js",
    "clean": "rm -rf build && mkdir build",
    "watch": "nodemon index.js --exec 'npm run flow && npm run lint && node'",
    "flow": "flow ./src",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "start": "npm run build && node --harmony ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kanuny/art_pwnz_server.git"
  },
  "author": "andreykanuny@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kanuny/art_pwnz_server/issues"
  },
  "engines": {
    "node": ">=7.2.x"
  },
  "homepage": "https://github.com/Kanuny/art_pwnz_server#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-node7": "^1.5.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.7.1",
    "eslint-config-node": "^1.6.0",
    "file-type": "^3.9.0",
    "flow-bin": "^0.33.0",
    "jimp": "^0.2.27",
    "jsonwebtoken": "^7.3.0",
    "kcors": "^2.2.0",
    "koa": "^2.0.0-alpha.7",
    "koa-body": "^2.0.0",
    "koa-bodyparser": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.5",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.0.1",
    "lodash": "^4.17.4",
    "mysql": "^2.13.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "sequelize": "^3.24.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "eslint": "^3.7.1",
    "eslint-config-node": "^1.6.0",
    "flow-bin": "^0.33.0"
  }
}
